{
  "name": "isomorphic-react-seed",
  "description": "isomorphic-react-seed",
  "version": "0.0.1",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuluyi/Isomorphic-React-Seed"
  },
  "homepage": "https://github.com/yuluyi/Isomorphic-React-Seed",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa",
    "transmit"
  ],
  "main": "dist/server.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "rimraf ./dist && concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "rimraf ./dist && concurrently --kill-others --raw \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "clean": "rimraf ./dist",
    "upload": "node --eval \"try{require('./scripts/upload.js')()}catch(err) {console.log(err.stack)}\"",
    "deploy-test": "sh -e scripts/deployTest.sh",
    "deploy-to-production-server-i-confirm": "sh -e scripts/deploy.sh"
  },
  "dependencies": {
    "babel-polyfill": "6.5.0",
    "classnames": "^2.2.5",
    "continuation-local-storage": "^3.1.6",
    "domready": "^1.0.8",
    "file-loader": "0.8.5",
    "ground-control": "^0.4.2",
    "ip": "^1.1.2",
    "isomorphic-fetch": "2.2.1",
    "isomorphic-style-loader": "^1.0.0",
    "koa": "1.1.2",
    "koa-add-trailing-slashes": "^1.1.0",
    "koa-compress": "^1.0.9",
    "koa-conditional-get": "^1.0.3",
    "koa-proxy": "0.4.1",
    "koa-send": "~3.1.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.6.1",
    "moment": "^2.13.0",
    "mz": "^2.3.1",
    "qrcode-react": "git+https://github.com/TapasFE/qrcode-react.git",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-iframe": "0.0.4",
    "react-inline-css": "2.1.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "react-transmit": "3.1.7",
    "redux": "^3.3.1",
    "redux-act": "^0.4.0",
    "redux-loop": "^1.1.0",
    "react-swipe": "^5.0.3",
    "redux-thunk": "^1.0.3",
    "source-map-support": "^0.4.0",
    "svg-react-loader": "^0.3.3",
    "swipe-js-iso": "^2.0.3",
    "url-join": "0.0.1",
    "uuid": "^2.0.2",
    "zenscroll": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autotrack": "^0.6.5",
    "babel": "6.5.2",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-proto-to-assign": "^6.9.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
